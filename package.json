{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@ant-design/pro-layout": "^6.4.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@umijs/preset-react": "^1.5.21",
    "@umijs/test": "^3.2.14",
    "antd": "^4.0.0",
    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "lint-staged": "^10.0.7",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "omit.js": "^2.0.2",
    "prettier": "^1.19.1",
    "qs": "^6.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^4.3.1",
    "umi": "^3.2.14",
    "umi-request": "^1.0.0",
    "use-merge-value": "^1.0.2",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-ui": "^2.2.6"
  }
}
